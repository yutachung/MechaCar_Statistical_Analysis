Suspension_table <- read.csv(file='Suspension_Coil.csv',check.names=F,stringsAsFactors = F)
head(Suspension_table)
total_summary <- summarize(Suspension_table$PSI)
?summarize()
total_summary <- Suspension_table %>% summarize(Mean=mean(PSI), Median=median(PSI), SD=sd(PSI), .groups='drop')
View(total_summary)
total_summary <- Suspension_table %>% summarize(Mean=mean(PSI), Median=median(PSI),Variance=mad(PSI), SD=sd(PSI), .groups='drop')
